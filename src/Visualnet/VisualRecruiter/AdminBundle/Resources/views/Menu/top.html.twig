<div><a id="logout" href="{{ path('AdminBundle_logout') }}" title="Wyloguj">{% trans %}wyloguj{% endtrans %}</a><img src="{{ asset('images/logout.png') }}" alt="" /></div>
<div><a class="modal-handler" data-href="{{ url('AdminBundle_guide') }}" data-height="350" data-width="340" data-title="Przewodnik" href="#" title="Przewodnik">{% trans %}przewodnik{% endtrans %}</a></div>
<div><a class="modal-handler" data-href="{{ url('UserBundle_change_password', {'id' : app.user.getId }) }}" data-height="200" data-width="400" data-title="Hasło" href="#" >{% trans %}zmień hasło{% endtrans %}</a></div>
<div><a class="modal-handler" data-href="{{ url('UserBundle_user_edit', {'id' : app.user.getId }) }}" data-height="320" data-width="600" data-title="Użytkownik" href="#" >{% trans %}konto{% endtrans %}</a></div>
<div><a class="modal-handler" data-href="{{ url('AdminBundle_suggest') }}" data-height="430" data-width="440" data-title="Pytania i sugestie" href="#" title="Pytania i sugestie"><span class="bold">{% trans %}pytania i sugestie{% endtrans %}</span></a></div>
<div class="logged-text">{% trans %}jesteś zalogowany jako{% endtrans %}: <span style="font-weight: bold;">{{ app.user.getName }} {{ app.user.getSurname }} ( {{ app.user.getUsername }} )</span> </div>


